        "wpackagist-plugin/varnish-http-purge": "^4.1.1",
        "wpackagist-plugin/wp-nested-pages": "3.1.4",
        "wpackagist-plugin/mce-table-buttons": "^3.0.0",
        "wpackagist-plugin/user-switching" : ">=1.5.1",
        "wpackagist-plugin/autodescription" : ">=2.9.0",
