    "prefer-stable": true,
    "minimum-stability" : "dev",
    "repositories": [
        {
            "type":"composer",
            "url":"https://packagist.org"
        },
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        },
        {
            "type":"package",
            "package": {
                "name": "webdevstudios/wp-search-with-algolia",
                "version":"master",
                "source": {
                    "url": "https://github.com/WebDevStudios/wp-search-with-algolia.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],

